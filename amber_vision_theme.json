{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Amber Vision",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Amber Vision",
            "appearance": "dark",
            "style": {
                "background": "#1a1408",
                "foreground": "#ffe8c0",
                "editor.background": "#1a1408",
                "editor.foreground": "#ffe8c0",
                "editor.line_number": "#6d5522",
                "editor.active_line_number": "#ffcc66",
                "editor.active_line.background": "#221a0a",
                "editor.gutter.background": "#1a1408",
                "border": "#3d2a11",
                "border.variant": "#221a0a",
                "text": "#ffe8c0",
                "text.muted": "#aa8855",
                "text.placeholder": "#6d5522",
                "text.accent": "#ffaa33",
                "surface.background": "#110d05",
                "elevated_surface.background": "#2a1f0f",
                "element.background": "#3d2a11",
                "element.hover": "#4d3515",
                "element.active": "#5d4019",
                "element.selected": "#4d3311",
                "ghost_element.hover": "#4d3515",
                "ghost_element.selected": "#4d3311",
                "tab_bar.background": "#1a1408",
                "tab.inactive_background": "#1a1408",
                "tab.active_background": "#221a0a",
                "status_bar.background": "#1a1408",
                "title_bar.background": "#1a1408",
                "panel.background": "#110d05",
                "toolbar.background": "#1a1408",
                "scrollbar.thumb.background": "#3d2a11",
                "scrollbar.track.background": "#1a1408",
                "error": "#ff4444",
                "error.background": "#3d1111",
                "warning": "#ffdd66",
                "warning.background": "#3d2d11",
                "info": "#5588ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffdd66",
                "created": "#55ff88",
                "deleted": "#ff4444",
                "conflict": "#ff8844",
                "ignored": "#6d5522",
                "syntax": {
                    "comment": {
                        "color": "#886633",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#997744",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ffbb66"
                    },
                    "constant.builtin": {
                        "color": "#ffaa44"
                    },
                    "constructor": {
                        "color": "#ffaa55"
                    },
                    "function": {
                        "color": "#ffaa55",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff9933",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ffaa44"
                    },
                    "operator": {
                        "color": "#ffcc88"
                    },
                    "property": {
                        "color": "#ffddaa"
                    },
                    "punctuation": {
                        "color": "#ddaa77"
                    },
                    "punctuation.bracket": {
                        "color": "#ffcc88"
                    },
                    "punctuation.delimiter": {
                        "color": "#ddaa77"
                    },
                    "string": {
                        "color": "#ffeecc"
                    },
                    "string.escape": {
                        "color": "#ffaa44"
                    },
                    "string.regex": {
                        "color": "#ffaa55"
                    },
                    "tag": {
                        "color": "#ff9933"
                    },
                    "type": {
                        "color": "#ffcc77"
                    },
                    "variable": {
                        "color": "#ffe8c0"
                    },
                    "variable.special": {
                        "color": "#ff4444"
                    }
                }
            }
        }
    ]
}
