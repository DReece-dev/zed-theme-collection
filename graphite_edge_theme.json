{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Graphite Edge",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Graphite Edge",
            "appearance": "dark",
            "style": {
                "background": "#111314",
                "foreground": "#e5e8ea",
                "editor.background": "#111314",
                "editor.foreground": "#e5e8ea",
                "editor.line_number": "#4d5358",
                "editor.active_line_number": "#b0b8c0",
                "editor.active_line.background": "#181a1c",
                "editor.gutter.background": "#111314",
                "border": "#272a2d",
                "border.variant": "#181a1c",
                "text": "#e5e8ea",
                "text.muted": "#7d858d",
                "text.placeholder": "#4d5358",
                "text.accent": "#70a0d0",
                "surface.background": "#0c0d0e",
                "elevated_surface.background": "#1c1e20",
                "element.background": "#272a2d",
                "element.hover": "#2f3236",
                "element.active": "#373a3f",
                "element.selected": "#272f35",
                "ghost_element.hover": "#2f3236",
                "ghost_element.selected": "#272f35",
                "tab_bar.background": "#111314",
                "tab.inactive_background": "#111314",
                "tab.active_background": "#181a1c",
                "status_bar.background": "#111314",
                "title_bar.background": "#111314",
                "panel.background": "#0c0d0e",
                "toolbar.background": "#111314",
                "scrollbar.thumb.background": "#272a2d",
                "scrollbar.track.background": "#111314",
                "error": "#ff5566",
                "error.background": "#3d1a22",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#70a0d0",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffbb55",
                "created": "#55ff88",
                "deleted": "#ff5566",
                "conflict": "#ff8855",
                "ignored": "#4d5358",
                "syntax": {
                    "comment": {
                        "color": "#5d656d",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#6d757d",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#a0b0c0"
                    },
                    "constant.builtin": {
                        "color": "#8090a0"
                    },
                    "constructor": {
                        "color": "#90a8c0"
                    },
                    "function": {
                        "color": "#90a8c0",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#70a0d0",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#8090a0"
                    },
                    "operator": {
                        "color": "#b0c8e0"
                    },
                    "property": {
                        "color": "#c0d0e0"
                    },
                    "punctuation": {
                        "color": "#a0a8b0"
                    },
                    "punctuation.bracket": {
                        "color": "#b0c8e0"
                    },
                    "punctuation.delimiter": {
                        "color": "#a0a8b0"
                    },
                    "string": {
                        "color": "#d0d8e0"
                    },
                    "string.escape": {
                        "color": "#8090a0"
                    },
                    "string.regex": {
                        "color": "#90a8c0"
                    },
                    "tag": {
                        "color": "#70a0d0"
                    },
                    "type": {
                        "color": "#b0c8d0"
                    },
                    "variable": {
                        "color": "#e5e8ea"
                    },
                    "variable.special": {
                        "color": "#ff5566"
                    }
                }
            }
        }
    ]
}
