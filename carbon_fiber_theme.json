{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Carbon Fiber",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Carbon Fiber",
            "appearance": "dark",
            "style": {
                "background": "#0f0f0f",
                "foreground": "#e8e8e8",
                "editor.background": "#0f0f0f",
                "editor.foreground": "#e8e8e8",
                "editor.line_number": "#555555",
                "editor.active_line_number": "#cccccc",
                "editor.active_line.background": "#1a1a1a",
                "editor.gutter.background": "#0f0f0f",
                "border": "#2a2a2a",
                "border.variant": "#1a1a1a",
                "text": "#e8e8e8",
                "text.muted": "#888888",
                "text.placeholder": "#555555",
                "text.accent": "#00ddff",
                "surface.background": "#0a0a0a",
                "elevated_surface.background": "#1f1f1f",
                "element.background": "#2a2a2a",
                "element.hover": "#353535",
                "element.active": "#404040",
                "element.selected": "#2a3a3a",
                "ghost_element.hover": "#353535",
                "ghost_element.selected": "#2a3a3a",
                "tab_bar.background": "#0f0f0f",
                "tab.inactive_background": "#0f0f0f",
                "tab.active_background": "#1a1a1a",
                "status_bar.background": "#0f0f0f",
                "title_bar.background": "#0f0f0f",
                "panel.background": "#0a0a0a",
                "toolbar.background": "#0f0f0f",
                "scrollbar.thumb.background": "#2a2a2a",
                "scrollbar.track.background": "#0f0f0f",
                "error": "#ff4466",
                "error.background": "#3d1a1a",
                "warning": "#ffaa44",
                "warning.background": "#3d2a11",
                "info": "#44aaff",
                "info.background": "#11223d",
                "success": "#44ff88",
                "success.background": "#113d22",
                "modified": "#ffaa44",
                "created": "#44ff88",
                "deleted": "#ff4466",
                "conflict": "#ff8844",
                "ignored": "#555555",
                "syntax": {
                    "comment": {
                        "color": "#666666",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#777777",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ffbb66"
                    },
                    "constant.builtin": {
                        "color": "#ff8844"
                    },
                    "constructor": {
                        "color": "#66bbff"
                    },
                    "function": {
                        "color": "#66bbff",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff66aa",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff8844"
                    },
                    "operator": {
                        "color": "#00ddff"
                    },
                    "property": {
                        "color": "#88ccff"
                    },
                    "punctuation": {
                        "color": "#aaaaaa"
                    },
                    "punctuation.bracket": {
                        "color": "#00ddff"
                    },
                    "punctuation.delimiter": {
                        "color": "#aaaaaa"
                    },
                    "string": {
                        "color": "#88ffaa"
                    },
                    "string.escape": {
                        "color": "#ff8844"
                    },
                    "string.regex": {
                        "color": "#00ddff"
                    },
                    "tag": {
                        "color": "#ff66aa"
                    },
                    "type": {
                        "color": "#ffcc66"
                    },
                    "variable": {
                        "color": "#e8e8e8"
                    },
                    "variable.special": {
                        "color": "#ff4466"
                    }
                }
            }
        }
    ]
}
