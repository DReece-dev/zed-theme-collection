{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Sapphire Code",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Sapphire Code",
            "appearance": "dark",
            "style": {
                "background": "#0a0f1a",
                "foreground": "#d0e5ff",
                "editor.background": "#0a0f1a",
                "editor.foreground": "#d0e5ff",
                "editor.line_number": "#3d4d6d",
                "editor.active_line_number": "#88aaff",
                "editor.active_line.background": "#0d1422",
                "editor.gutter.background": "#0a0f1a",
                "border": "#1a2a3d",
                "border.variant": "#0d1422",
                "text": "#d0e5ff",
                "text.muted": "#6d88aa",
                "text.placeholder": "#3d4d6d",
                "text.accent": "#5588ff",
                "surface.background": "#050a11",
                "elevated_surface.background": "#0f1a2a",
                "element.background": "#1a2a3d",
                "element.hover": "#22354d",
                "element.active": "#2a405d",
                "element.selected": "#1a334d",
                "ghost_element.hover": "#22354d",
                "ghost_element.selected": "#1a334d",
                "tab_bar.background": "#0a0f1a",
                "tab.inactive_background": "#0a0f1a",
                "tab.active_background": "#0d1422",
                "status_bar.background": "#0a0f1a",
                "title_bar.background": "#0a0f1a",
                "panel.background": "#050a11",
                "toolbar.background": "#0a0f1a",
                "scrollbar.thumb.background": "#1a2a3d",
                "scrollbar.track.background": "#0a0f1a",
                "error": "#ff5577",
                "error.background": "#3d1a22",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#5599ff",
                "info.background": "#11223d",
                "success": "#55ff99",
                "success.background": "#113d2a",
                "modified": "#ffbb55",
                "created": "#55ff99",
                "deleted": "#ff5577",
                "conflict": "#ff9966",
                "ignored": "#3d4d6d",
                "syntax": {
                    "comment": {
                        "color": "#4d6688",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#5d7799",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#88bbff"
                    },
                    "constant.builtin": {
                        "color": "#5588ff"
                    },
                    "constructor": {
                        "color": "#66aaff"
                    },
                    "function": {
                        "color": "#66aaff",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#8899ff",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#5588ff"
                    },
                    "operator": {
                        "color": "#88ccff"
                    },
                    "property": {
                        "color": "#99ddff"
                    },
                    "punctuation": {
                        "color": "#99bbdd"
                    },
                    "punctuation.bracket": {
                        "color": "#88ccff"
                    },
                    "punctuation.delimiter": {
                        "color": "#99bbdd"
                    },
                    "string": {
                        "color": "#aaddff"
                    },
                    "string.escape": {
                        "color": "#5588ff"
                    },
                    "string.regex": {
                        "color": "#66aaff"
                    },
                    "tag": {
                        "color": "#8899ff"
                    },
                    "type": {
                        "color": "#99ccff"
                    },
                    "variable": {
                        "color": "#d0e5ff"
                    },
                    "variable.special": {
                        "color": "#ff5577"
                    }
                }
            }
        }
    ]
}
