{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Magenta Eclipse",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Magenta Eclipse",
            "appearance": "dark",
            "style": {
                "background": "#180a1a",
                "foreground": "#f0e0ff",
                "editor.background": "#180a1a",
                "editor.foreground": "#f0e0ff",
                "editor.line_number": "#5d3d6d",
                "editor.active_line_number": "#cc88ff",
                "editor.active_line.background": "#1f0d22",
                "editor.gutter.background": "#180a1a",
                "border": "#2d1a3d",
                "border.variant": "#1f0d22",
                "text": "#f0e0ff",
                "text.muted": "#9966aa",
                "text.placeholder": "#5d3d6d",
                "text.accent": "#dd66ff",
                "surface.background": "#100511",
                "elevated_surface.background": "#22112a",
                "element.background": "#2d1a3d",
                "element.hover": "#35224d",
                "element.active": "#3d2a5d",
                "element.selected": "#2d1f3d",
                "ghost_element.hover": "#35224d",
                "ghost_element.selected": "#2d1f3d",
                "tab_bar.background": "#180a1a",
                "tab.inactive_background": "#180a1a",
                "tab.active_background": "#1f0d22",
                "status_bar.background": "#180a1a",
                "title_bar.background": "#180a1a",
                "panel.background": "#100511",
                "toolbar.background": "#180a1a",
                "scrollbar.thumb.background": "#2d1a3d",
                "scrollbar.track.background": "#180a1a",
                "error": "#ff5588",
                "error.background": "#3d1a2a",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#5588ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffbb55",
                "created": "#55ff88",
                "deleted": "#ff5588",
                "conflict": "#ff8855",
                "ignored": "#5d3d6d",
                "syntax": {
                    "comment": {
                        "color": "#7755aa",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#8866bb",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ff99dd"
                    },
                    "constant.builtin": {
                        "color": "#ff77cc"
                    },
                    "constructor": {
                        "color": "#dd88ff"
                    },
                    "function": {
                        "color": "#dd88ff",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff66dd",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff77cc"
                    },
                    "operator": {
                        "color": "#ee88ff"
                    },
                    "property": {
                        "color": "#ffaaee"
                    },
                    "punctuation": {
                        "color": "#cc99dd"
                    },
                    "punctuation.bracket": {
                        "color": "#ee88ff"
                    },
                    "punctuation.delimiter": {
                        "color": "#cc99dd"
                    },
                    "string": {
                        "color": "#ffccee"
                    },
                    "string.escape": {
                        "color": "#ff77cc"
                    },
                    "string.regex": {
                        "color": "#dd88ff"
                    },
                    "tag": {
                        "color": "#ff66dd"
                    },
                    "type": {
                        "color": "#ffbbee"
                    },
                    "variable": {
                        "color": "#f0e0ff"
                    },
                    "variable.special": {
                        "color": "#ff5588"
                    }
                }
            }
        }
    ]
}
