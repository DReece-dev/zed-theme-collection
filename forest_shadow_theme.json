{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Forest Shadow",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Forest Shadow",
            "appearance": "dark",
            "style": {
                "background": "#0d1a0d",
                "foreground": "#e5ffe5",
                "editor.background": "#0d1a0d",
                "editor.foreground": "#e5ffe5",
                "editor.line_number": "#3d6d3d",
                "editor.active_line_number": "#88ff88",
                "editor.active_line.background": "#102210",
                "editor.gutter.background": "#0d1a0d",
                "border": "#1a3d1a",
                "border.variant": "#102210",
                "text": "#e5ffe5",
                "text.muted": "#6daa6d",
                "text.placeholder": "#3d6d3d",
                "text.accent": "#55ff55",
                "surface.background": "#081108",
                "elevated_surface.background": "#152a15",
                "element.background": "#1a3d1a",
                "element.hover": "#224d22",
                "element.active": "#2a5d2a",
                "element.selected": "#1a4d2a",
                "ghost_element.hover": "#224d22",
                "ghost_element.selected": "#1a4d2a",
                "tab_bar.background": "#0d1a0d",
                "tab.inactive_background": "#0d1a0d",
                "tab.active_background": "#102210",
                "status_bar.background": "#0d1a0d",
                "title_bar.background": "#0d1a0d",
                "panel.background": "#081108",
                "toolbar.background": "#0d1a0d",
                "scrollbar.thumb.background": "#1a3d1a",
                "scrollbar.track.background": "#0d1a0d",
                "error": "#ff5566",
                "error.background": "#3d1a1a",
                "warning": "#ffcc55",
                "warning.background": "#3d2d11",
                "info": "#5599ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffcc55",
                "created": "#55ff88",
                "deleted": "#ff5566",
                "conflict": "#ff9955",
                "ignored": "#3d6d3d",
                "syntax": {
                    "comment": {
                        "color": "#4d7d4d",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#5d8d5d",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#aaffaa"
                    },
                    "constant.builtin": {
                        "color": "#88ff88"
                    },
                    "constructor": {
                        "color": "#77dd77"
                    },
                    "function": {
                        "color": "#77dd77",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#99ff66",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#88ff88"
                    },
                    "operator": {
                        "color": "#66ffaa"
                    },
                    "property": {
                        "color": "#88ffcc"
                    },
                    "punctuation": {
                        "color": "#99cc99"
                    },
                    "punctuation.bracket": {
                        "color": "#66ffaa"
                    },
                    "punctuation.delimiter": {
                        "color": "#99cc99"
                    },
                    "string": {
                        "color": "#ccffaa"
                    },
                    "string.escape": {
                        "color": "#88ff88"
                    },
                    "string.regex": {
                        "color": "#77dd77"
                    },
                    "tag": {
                        "color": "#99ff66"
                    },
                    "type": {
                        "color": "#ddff88"
                    },
                    "variable": {
                        "color": "#e5ffe5"
                    },
                    "variable.special": {
                        "color": "#ff5566"
                    }
                }
            }
        }
    ]
}
