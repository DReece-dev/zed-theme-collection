{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Midnight Crimson",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Midnight Crimson",
            "appearance": "dark",
            "style": {
                "background": "#1a0a0a",
                "foreground": "#ffe5e5",
                "editor.background": "#1a0a0a",
                "editor.foreground": "#ffe5e5",
                "editor.line_number": "#664444",
                "editor.active_line_number": "#ff8888",
                "editor.active_line.background": "#220d0d",
                "editor.gutter.background": "#1a0a0a",
                "border": "#3d1a1a",
                "border.variant": "#220d0d",
                "text": "#ffe5e5",
                "text.muted": "#aa6666",
                "text.placeholder": "#664444",
                "text.accent": "#ff6666",
                "surface.background": "#110505",
                "elevated_surface.background": "#2a1111",
                "element.background": "#3d1a1a",
                "element.hover": "#4d2222",
                "element.active": "#5d2a2a",
                "element.selected": "#4d1f1f",
                "ghost_element.hover": "#4d2222",
                "ghost_element.selected": "#4d1f1f",
                "tab_bar.background": "#1a0a0a",
                "tab.inactive_background": "#1a0a0a",
                "tab.active_background": "#220d0d",
                "status_bar.background": "#1a0a0a",
                "title_bar.background": "#1a0a0a",
                "panel.background": "#110505",
                "toolbar.background": "#1a0a0a",
                "scrollbar.thumb.background": "#3d1a1a",
                "scrollbar.track.background": "#1a0a0a",
                "error": "#ff4444",
                "error.background": "#3d0000",
                "warning": "#ffaa44",
                "warning.background": "#3d2200",
                "info": "#4488ff",
                "info.background": "#001a3d",
                "success": "#44ff88",
                "success.background": "#003d1a",
                "modified": "#ffaa44",
                "created": "#44ff88",
                "deleted": "#ff4444",
                "conflict": "#ff8844",
                "ignored": "#664444",
                "syntax": {
                    "comment": {
                        "color": "#775555",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#886666",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ffaa88"
                    },
                    "constant.builtin": {
                        "color": "#ff8866"
                    },
                    "constructor": {
                        "color": "#ff99cc"
                    },
                    "function": {
                        "color": "#ff99cc",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff6666",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff8866"
                    },
                    "operator": {
                        "color": "#ff99ff"
                    },
                    "property": {
                        "color": "#ffaadd"
                    },
                    "punctuation": {
                        "color": "#ddaaaa"
                    },
                    "punctuation.bracket": {
                        "color": "#ff99ff"
                    },
                    "punctuation.delimiter": {
                        "color": "#ddaaaa"
                    },
                    "string": {
                        "color": "#ffccaa"
                    },
                    "string.escape": {
                        "color": "#ff8866"
                    },
                    "string.regex": {
                        "color": "#ffaaff"
                    },
                    "tag": {
                        "color": "#ff6666"
                    },
                    "type": {
                        "color": "#ffbb88"
                    },
                    "variable": {
                        "color": "#ffe5e5"
                    },
                    "variable.special": {
                        "color": "#ff4444"
                    }
                }
            }
        }
    ]
}
