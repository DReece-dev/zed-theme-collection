{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Volcanic Ash",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Volcanic Ash - 2",
            "appearance": "dark",
            "style": {
                "background": "#1a1515",
                "foreground": "#f0e0d0",
                "editor.background": "#1a1515",
                "editor.foreground": "#f0e0d0",
                "editor.line_number": "#6d5555",
                "editor.active_line_number": "#ffaa88",
                "editor.active_line.background": "#221a1a",
                "editor.gutter.background": "#1a1515",
                "border": "#3d2a2a",
                "border.variant": "#221a1a",
                "text": "#f0e0d0",
                "text.muted": "#aa8877",
                "text.placeholder": "#6d5555",
                "text.accent": "#ff8855",
                "surface.background": "#110d0d",
                "elevated_surface.background": "#2a2020",
                "element.background": "#3d2a2a",
                "element.hover": "#4d3535",
                "element.active": "#5d4040",
                "element.selected": "#4d3333",
                "ghost_element.hover": "#4d3535",
                "ghost_element.selected": "#4d3333",
                "tab_bar.background": "#1a1515",
                "tab.inactive_background": "#1a1515",
                "tab.active_background": "#221a1a",
                "status_bar.background": "#1a1515",
                "title_bar.background": "#1a1515",
                "panel.background": "#110d0d",
                "toolbar.background": "#1a1515",
                "scrollbar.thumb.background": "#3d2a2a",
                "scrollbar.track.background": "#1a1515",
                "error": "#ff4455",
                "error.background": "#3d1111",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#5588ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffbb55",
                "created": "#55ff88",
                "deleted": "#ff4455",
                "conflict": "#ff8855",
                "ignored": "#6d5555",
                "syntax": {
                    "comment": {
                        "color": "#886666",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#997777",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ffaa77"
                    },
                    "constant.builtin": {
                        "color": "#ff8855"
                    },
                    "constructor": {
                        "color": "#ff9966"
                    },
                    "function": {
                        "color": "#ff9966",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff7755",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff8855"
                    },
                    "operator": {
                        "color": "#ffbb88"
                    },
                    "property": {
                        "color": "#ffaa99"
                    },
                    "punctuation": {
                        "color": "#ccaa99"
                    },
                    "punctuation.bracket": {
                        "color": "#ffbb88"
                    },
                    "punctuation.delimiter": {
                        "color": "#ccaa99"
                    },
                    "string": {
                        "color": "#ffcc99"
                    },
                    "string.escape": {
                        "color": "#ff8855"
                    },
                    "string.regex": {
                        "color": "#ff9966"
                    },
                    "tag": {
                        "color": "#ff7755"
                    },
                    "type": {
                        "color": "#ffbb77"
                    },
                    "variable": {
                        "color": "#f0e0d0"
                    },
                    "variable.special": {
                        "color": "#ff4455"
                    }
                }
            }
        }
    ]
}
