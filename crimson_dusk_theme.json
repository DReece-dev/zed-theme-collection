{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Crimson Dusk",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Crimson Dusk",
            "appearance": "dark",
            "style": {
                "background": "#1a0d11",
                "foreground": "#ffd5e0",
                "editor.background": "#1a0d11",
                "editor.foreground": "#ffd5e0",
                "editor.line_number": "#6d3d4d",
                "editor.active_line_number": "#ff8899",
                "editor.active_line.background": "#221015",
                "editor.gutter.background": "#1a0d11",
                "border": "#3d1a22",
                "border.variant": "#221015",
                "text": "#ffd5e0",
                "text.muted": "#aa6d7d",
                "text.placeholder": "#6d3d4d",
                "text.accent": "#ff5577",
                "surface.background": "#110508",
                "elevated_surface.background": "#2a1518",
                "element.background": "#3d1a22",
                "element.hover": "#4d222a",
                "element.active": "#5d2a32",
                "element.selected": "#4d1f27",
                "ghost_element.hover": "#4d222a",
                "ghost_element.selected": "#4d1f27",
                "tab_bar.background": "#1a0d11",
                "tab.inactive_background": "#1a0d11",
                "tab.active_background": "#221015",
                "status_bar.background": "#1a0d11",
                "title_bar.background": "#1a0d11",
                "panel.background": "#110508",
                "toolbar.background": "#1a0d11",
                "scrollbar.thumb.background": "#3d1a22",
                "scrollbar.track.background": "#1a0d11",
                "error": "#ff3355",
                "error.background": "#3d0011",
                "warning": "#ffaa55",
                "warning.background": "#3d2211",
                "info": "#5588ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffaa55",
                "created": "#55ff88",
                "deleted": "#ff3355",
                "conflict": "#ff7755",
                "ignored": "#6d3d4d",
                "syntax": {
                    "comment": {
                        "color": "#884d5d",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#995d6d",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ff9988"
                    },
                    "constant.builtin": {
                        "color": "#ff7766"
                    },
                    "constructor": {
                        "color": "#ff8899"
                    },
                    "function": {
                        "color": "#ff8899",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff5577",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff7766"
                    },
                    "operator": {
                        "color": "#ffaabb"
                    },
                    "property": {
                        "color": "#ffbbaa"
                    },
                    "punctuation": {
                        "color": "#dd9999"
                    },
                    "punctuation.bracket": {
                        "color": "#ffaabb"
                    },
                    "punctuation.delimiter": {
                        "color": "#dd9999"
                    },
                    "string": {
                        "color": "#ffccbb"
                    },
                    "string.escape": {
                        "color": "#ff7766"
                    },
                    "string.regex": {
                        "color": "#ff8899"
                    },
                    "tag": {
                        "color": "#ff5577"
                    },
                    "type": {
                        "color": "#ffaa99"
                    },
                    "variable": {
                        "color": "#ffd5e0"
                    },
                    "variable.special": {
                        "color": "#ff3355"
                    }
                }
            }
        }
    ]
}
