{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Electric Purple",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Electric Purple",
            "appearance": "dark",
            "style": {
                "background": "#0d0a1a",
                "foreground": "#f0e5ff",
                "editor.background": "#0d0a1a",
                "editor.foreground": "#f0e5ff",
                "editor.line_number": "#4d3d6d",
                "editor.active_line_number": "#bb88ff",
                "editor.active_line.background": "#120d22",
                "editor.gutter.background": "#0d0a1a",
                "border": "#2a1a3d",
                "border.variant": "#120d22",
                "text": "#f0e5ff",
                "text.muted": "#8866aa",
                "text.placeholder": "#4d3d6d",
                "text.accent": "#bb66ff",
                "surface.background": "#080511",
                "elevated_surface.background": "#1a1128",
                "element.background": "#2a1a3d",
                "element.hover": "#33224d",
                "element.active": "#3d2a5d",
                "element.selected": "#2a1f3d",
                "ghost_element.hover": "#33224d",
                "ghost_element.selected": "#2a1f3d",
                "tab_bar.background": "#0d0a1a",
                "tab.inactive_background": "#0d0a1a",
                "tab.active_background": "#120d22",
                "status_bar.background": "#0d0a1a",
                "title_bar.background": "#0d0a1a",
                "panel.background": "#080511",
                "toolbar.background": "#0d0a1a",
                "scrollbar.thumb.background": "#2a1a3d",
                "scrollbar.track.background": "#0d0a1a",
                "error": "#ff5588",
                "error.background": "#3d1a2a",
                "warning": "#ffbb44",
                "warning.background": "#3d2a11",
                "info": "#44bbff",
                "info.background": "#11223d",
                "success": "#44ff88",
                "success.background": "#113d22",
                "modified": "#ffbb44",
                "created": "#44ff88",
                "deleted": "#ff5588",
                "conflict": "#ff8844",
                "ignored": "#4d3d6d",
                "syntax": {
                    "comment": {
                        "color": "#6655aa",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#7766bb",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#ffaa88"
                    },
                    "constant.builtin": {
                        "color": "#ff8866"
                    },
                    "constructor": {
                        "color": "#bb88ff"
                    },
                    "function": {
                        "color": "#bb88ff",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#ff66dd",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#ff8866"
                    },
                    "operator": {
                        "color": "#dd66ff"
                    },
                    "property": {
                        "color": "#ffaaff"
                    },
                    "punctuation": {
                        "color": "#bb99dd"
                    },
                    "punctuation.bracket": {
                        "color": "#dd66ff"
                    },
                    "punctuation.delimiter": {
                        "color": "#bb99dd"
                    },
                    "string": {
                        "color": "#aaffdd"
                    },
                    "string.escape": {
                        "color": "#ff8866"
                    },
                    "string.regex": {
                        "color": "#88ddff"
                    },
                    "tag": {
                        "color": "#ff66dd"
                    },
                    "type": {
                        "color": "#ffcc88"
                    },
                    "variable": {
                        "color": "#f0e5ff"
                    },
                    "variable.special": {
                        "color": "#ff5588"
                    }
                }
            }
        }
    ]
}
