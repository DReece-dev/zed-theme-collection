{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Slate Command",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Slate Command",
            "appearance": "dark",
            "style": {
                "background": "#12151a",
                "foreground": "#e8ecf0",
                "editor.background": "#12151a",
                "editor.foreground": "#e8ecf0",
                "editor.line_number": "#444d5d",
                "editor.active_line_number": "#99aabb",
                "editor.active_line.background": "#171c22",
                "editor.gutter.background": "#12151a",
                "border": "#222a35",
                "border.variant": "#171c22",
                "text": "#e8ecf0",
                "text.muted": "#7788aa",
                "text.placeholder": "#444d5d",
                "text.accent": "#5599cc",
                "surface.background": "#0d1014",
                "elevated_surface.background": "#1a2028",
                "element.background": "#222a35",
                "element.hover": "#2a3544",
                "element.active": "#324053",
                "element.selected": "#223344",
                "ghost_element.hover": "#2a3544",
                "ghost_element.selected": "#223344",
                "tab_bar.background": "#12151a",
                "tab.inactive_background": "#12151a",
                "tab.active_background": "#171c22",
                "status_bar.background": "#12151a",
                "title_bar.background": "#12151a",
                "panel.background": "#0d1014",
                "toolbar.background": "#12151a",
                "scrollbar.thumb.background": "#222a35",
                "scrollbar.track.background": "#12151a",
                "error": "#ff5566",
                "error.background": "#3d1a22",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#5599cc",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffbb55",
                "created": "#55ff88",
                "deleted": "#ff5566",
                "conflict": "#ff8855",
                "ignored": "#444d5d",
                "syntax": {
                    "comment": {
                        "color": "#556677",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#667788",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#88aadd"
                    },
                    "constant.builtin": {
                        "color": "#6688cc"
                    },
                    "constructor": {
                        "color": "#7799cc"
                    },
                    "function": {
                        "color": "#7799cc",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#6699dd",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#6688cc"
                    },
                    "operator": {
                        "color": "#99bbdd"
                    },
                    "property": {
                        "color": "#aaccee"
                    },
                    "punctuation": {
                        "color": "#aabbcc"
                    },
                    "punctuation.bracket": {
                        "color": "#99bbdd"
                    },
                    "punctuation.delimiter": {
                        "color": "#aabbcc"
                    },
                    "string": {
                        "color": "#bbddee"
                    },
                    "string.escape": {
                        "color": "#6688cc"
                    },
                    "string.regex": {
                        "color": "#7799cc"
                    },
                    "tag": {
                        "color": "#6699dd"
                    },
                    "type": {
                        "color": "#99ccee"
                    },
                    "variable": {
                        "color": "#e8ecf0"
                    },
                    "variable.special": {
                        "color": "#ff5566"
                    }
                }
            }
        }
    ]
}
