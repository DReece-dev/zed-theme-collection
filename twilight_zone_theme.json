{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Twilight Zone",
    "author": "Dylan Reece",
    "themes": [
        {
            "name": "Twilight Zone",
            "appearance": "dark",
            "style": {
                "background": "#0f0a1a",
                "foreground": "#e8d5ff",
                "editor.background": "#0f0a1a",
                "editor.foreground": "#e8d5ff",
                "editor.line_number": "#4d3d6d",
                "editor.active_line_number": "#bb88ff",
                "editor.active_line.background": "#140d22",
                "editor.gutter.background": "#0f0a1a",
                "border": "#261a3d",
                "border.variant": "#140d22",
                "text": "#e8d5ff",
                "text.muted": "#8866aa",
                "text.placeholder": "#4d3d6d",
                "text.accent": "#aa66ff",
                "surface.background": "#0a0511",
                "elevated_surface.background": "#1a112a",
                "element.background": "#261a3d",
                "element.hover": "#2e224d",
                "element.active": "#362a5d",
                "element.selected": "#261f3d",
                "ghost_element.hover": "#2e224d",
                "ghost_element.selected": "#261f3d",
                "tab_bar.background": "#0f0a1a",
                "tab.inactive_background": "#0f0a1a",
                "tab.active_background": "#140d22",
                "status_bar.background": "#0f0a1a",
                "title_bar.background": "#0f0a1a",
                "panel.background": "#0a0511",
                "toolbar.background": "#0f0a1a",
                "scrollbar.thumb.background": "#261a3d",
                "scrollbar.track.background": "#0f0a1a",
                "error": "#ff5588",
                "error.background": "#3d1a2a",
                "warning": "#ffbb55",
                "warning.background": "#3d2a11",
                "info": "#5588ff",
                "info.background": "#11223d",
                "success": "#55ff88",
                "success.background": "#113d22",
                "modified": "#ffbb55",
                "created": "#55ff88",
                "deleted": "#ff5588",
                "conflict": "#ff8855",
                "ignored": "#4d3d6d",
                "syntax": {
                    "comment": {
                        "color": "#6655aa",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#7766bb",
                        "font_style": "italic"
                    },
                    "constant": {
                        "color": "#dd99ff"
                    },
                    "constant.builtin": {
                        "color": "#bb77ff"
                    },
                    "constructor": {
                        "color": "#cc88ff"
                    },
                    "function": {
                        "color": "#cc88ff",
                        "font_weight": 700
                    },
                    "keyword": {
                        "color": "#aa66ff",
                        "font_weight": 700
                    },
                    "number": {
                        "color": "#bb77ff"
                    },
                    "operator": {
                        "color": "#dd99ff"
                    },
                    "property": {
                        "color": "#eeaaff"
                    },
                    "punctuation": {
                        "color": "#bb99dd"
                    },
                    "punctuation.bracket": {
                        "color": "#dd99ff"
                    },
                    "punctuation.delimiter": {
                        "color": "#bb99dd"
                    },
                    "string": {
                        "color": "#ffbbee"
                    },
                    "string.escape": {
                        "color": "#bb77ff"
                    },
                    "string.regex": {
                        "color": "#cc88ff"
                    },
                    "tag": {
                        "color": "#aa66ff"
                    },
                    "type": {
                        "color": "#eebb ff"
                    },
                    "variable": {
                        "color": "#e8d5ff"
                    },
                    "variable.special": {
                        "color": "#ff5588"
                    }
                }
            }
        }
    ]
}
